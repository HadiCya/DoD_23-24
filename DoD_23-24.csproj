    <None Remove="Content\LevelTiles\LightShadow_pipo.png" />
    <None Remove="Content\LevelTiles\LightShadow_pipo.tsx" />
    <None Remove="Content\LevelTiles\samplemap.tmx" />
    <None Remove="Content\2D\Sprites\Special1.png" />
    <None Remove="Content\2D\Sprites\Item.png" />
    <None Remove="Content\2D\Sprites\DialogueBox.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Icon.ico" />
    <EmbeddedResource Include="Icon.bmp" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="MonoGame.Framework.DesktopGL" Version="3.8.1.303" />
