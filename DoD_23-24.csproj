    <None Remove="Content\Fonts\" />
    <None Remove="Content\LevelTiles\" />
    <None Remove="Content\LevelTiles\[A]Dirt_pipo.png" />
    <None Remove="Content\LevelTiles\[A]Dirt_pipo.tsx" />
    <None Remove="Content\LevelTiles\[A]Flower_pipo.png" />
    <None Remove="Content\LevelTiles\[A]Flower_pipo.tsx" />
    <None Remove="Content\LevelTiles\[A]Grass_pipo.png" />
